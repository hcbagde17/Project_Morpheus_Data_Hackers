{
  "name": "proctorwatch",
  "private": true,
  "version": "1.0.0",
  "description": "AI-Based Student Proctoring & Examination Management System",
  "main": "electron/main.cjs",
  "author": "ProctorWatch Team",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "vite build && electron-builder",
    "seed": "node supabase/seed.mjs"
  },
  "build": {
    "appId": "com.proctorwatch.app",
    "productName": "ProctorWatch",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    },
    "publish": {
      "provider": "generic",
      "url": "https://example.com/auto-updates"
    }
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.4",
    "concurrently": "^9.2.1",
    "electron": "^40.4.0",
    "electron-builder": "^26.7.0",
    "vite": "^7.3.1",
    "wait-on": "^9.0.3"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fullcalendar/core": "^6.1.20",
    "@fullcalendar/daygrid": "^6.1.20",
    "@fullcalendar/react": "^6.1.20",
    "@hookform/resolvers": "^5.2.2",
    "@mediapipe/tasks-vision": "^0.10.32",
    "@mui/icons-material": "^7.3.8",
    "@mui/material": "^7.3.8",
    "@ricky0123/vad-web": "^0.0.30",
    "@supabase/supabase-js": "^2.95.3",
    "@tensorflow-models/coco-ssd": "^2.2.3",
    "@tensorflow/tfjs": "^4.22.0",
    "date-fns": "^4.1.0",
    "electron-updater": "^6.1.7",
    "face-api.js": "^0.22.2",
    "koffi": "^2.15.1",
    "mermaid": "^10.9.0",
    "onnxruntime-web": "^1.25.0-dev.20260209-a3749f1353",
    "pdfjs-dist": "^5.4.624",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.71.1",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0",
    "systeminformation": "^5.31.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  }
}
